{% for block in section.blocks %}
	{% if block.type == 'preload_image' and block.settings.image %}
	  {% render 'responsive-preload', image: block.settings.image %}
	{% endif %}
  {% endfor %}
  
  {% schema %}
  {
	"name": "Preload Images",
	"max_blocks": 5,
	"blocks": [
	  {
		"type": "preload_image",
		"name": "Preload Image",
		"settings": [
		  {
			"type": "image_picker",
			"id": "image",
			"label": "Image"
		  }
		]
	  }
	]
  }
  {% endschema %}